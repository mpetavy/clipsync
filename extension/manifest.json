{
  "manifest_version": 3,
  "name": "Clipsync",
  "version": "1.0.0",
  "description": "Syncs bookmarks",
  "permissions": ["bookmarks", "storage", "alarms"],
  "host_permissions": ["http://*", "https://*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
